{
  "name": "react-teleporter",
  "description": "Teleport React components in the same React tree.",
  "version": "2.2.1",
  "keywords": [
    "react",
    "portal",
    "teleport",
    "tunnel"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/gregberge"
  },
  "repository": "git@github.com:gregberge/react-teleporter.git",
  "sideEffects": false,
  "main": "dist/react-teleporter.cjs.js",
  "module": "dist/react-teleporter.es.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write \"**/*.{js,json,md,mdx}\"",
    "lint": "eslint .",
    "prepublishOnly": "yarn build",
    "release": "standard-version && conventional-github-releaser --preset angular",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "codecov": "^3.8.2",
    "conventional-github-releaser": "^3.1.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.52.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.0"
  }
